<header>
  <h1>Sign In</h1>
</header>

<%= form_tag(session_path) do %>
<fieldset>
  <p>
    <%= email_field_tag :email, nil, autofocus: true, placeholder: "Email" %>
  </p>
  <p>
    <%= password_field_tag :password, nil, placeholder: "Password"%>
  </p>
  <%= submit_tag "Sign In" %>
</fieldset>
<% end %>
